{
    "version": 2,
    "builds": [
        {
            "src": "vite.config.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/frontend/(.*)",
            "dest": "/frontend/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/backend/$1"
        }
    ]
}