{
    "version": 2,
    "builds": [
        {
            "src": "/frontend",
            "use": "@vercel/static"
        },
        {
            "src": "/server",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}